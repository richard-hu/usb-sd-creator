msgctxt "Creator|"
msgid "LibreELEC USB-SD Creator"
msgstr "LibreELEC USB-SD készítő"

msgctxt "Creator|"
msgid "Write image"
msgstr "Lemezkép írása"

msgctxt "Creator|"
msgid "Select your USB stick or SD card"
msgstr "USB meghajtó vagy SD kártya kiválasztása"

msgctxt "Creator|"
msgid "Download or select a local image file"
msgstr "Letöltés vagy helyi lemezkép kiválasztása"

msgctxt "Creator|"
msgid "Eject device"
msgstr "Lemez kiadása"

msgctxt "Creator|"
msgid "Load device"
msgstr "Lemez betöltése"

msgctxt "Creator|"
msgid ""
"Root privileges required to write image.\n"
"Run application with sudo."
msgstr ""
"Rendszergazdai jogosultságok szükséges az íráshoz.\n"
"Futtasd az alkalmazást sudo -val."

#, qt-format
msgctxt "Creator|"
msgid ""
"Version: %1\n"
"Build date: %2"
msgstr ""
"Verzió %1\n"
"Build dátum: %2"

msgctxt "Creator|"
msgid "This software was created with love and released"
msgstr "Ez a program szeretettel lett  létrehozva és kiadott"

msgctxt "Creator|"
msgid "under GPLv2, using earlier work from RasPlex."
msgstr "GPLv2 alatt, használva RasPlex korábbi munkáját."

msgctxt "Creator|"
msgid "For license, credits and history, please read:"
msgstr "A licenszért, közreműködőkért és a történelemért kérjük olvassa el:"

msgctxt "Creator|"
msgid "If you enjoy using LibreELEC please consider a"
msgstr "Ha tetszik LibreELEC kérjük"

msgctxt "Creator|"
msgid "donation to support the project."
msgstr "adományozzon a projekt támogátására."

msgctxt "Creator|"
msgid "Click the logo below or donate"
msgstr "Kattints az alábbi logora vagy adományozz"

msgctxt "Creator|"
msgid "using Paypal to:"
msgstr "Paypalt használva hogy:"

msgctxt "Creator|"
msgid "[Stable]"
msgstr "[Stabil]"

msgctxt "Creator|"
msgid "[Alpha]"
msgstr "[Alfa]"

msgctxt "Creator|"
msgid "[Beta]"
msgstr "[Béta]"

msgctxt "Creator|"
msgid "Download complete, syncing file..."
msgstr "Letöltés kész, fájl szinkronizálása..."

msgctxt "Creator|"
msgid "Download complete, verifying checksum..."
msgstr "Letöltés kész, checksum ellenőrzése..."

msgctxt "Creator|"
msgid "Download complete, checksum ok."
msgstr "Letöltés kész, checksum rendben."

msgctxt "Creator|"
msgid "Download complete, checksum not ok."
msgstr "Letöltés kész, checksum hibás."

#, qt-format
msgctxt "Creator|"
msgid "%1 seconds remaining - %2% at %3"
msgstr "%1 másodperc van hátra - %2% a %3 -ból"

msgctxt "Creator|"
msgid "Download canceled."
msgstr "Letöltés megszakítva."

msgctxt "Creator|"
msgid "Directory to store image file"
msgstr "Könyvtár a képfájl tárolásához"

msgctxt "Creator|"
msgid "Error"
msgstr "Hiba"

#, qt-format
msgctxt "Creator|"
msgid ""
"File \n"
"%1/%2\n"
"already exist."
msgstr ""
"A \n"
"%1/%2\n"
"fájl már létezik."

msgctxt "Creator|"
msgid "Do you want to overwrite?"
msgstr "Biztosan felül akarod írni?"

msgctxt "Creator|"
msgid "File already exists."
msgstr "A fájl már létezik."

msgctxt "Creator|"
msgid "Failed to open file for writing!"
msgstr "Nem sikerült a fájlt megnyitni az íráshoz!"

msgctxt "Creator|"
msgid "Open image file"
msgstr "Lemezkép megnyitása"

msgctxt "Creator|"
msgid ""
"Compressed gz image (*img.gz);;Compressed zip image (*img.zip);;Uncompressed "
"image (*.img);;All files (*.*)"
msgstr ""
"Gz tömörített képfájl (*img.gz);;Zip tömörített képfájl "
"(*img.zip);;Tömörítetlen képfájl (*.img);;Minden fájl (*.*)"

msgctxt "Creator|"
msgid "Writing canceled."
msgstr "Írás megszakítva."

msgctxt "Creator|"
msgid "Confirm write"
msgstr "Írás megerősítésa"

#, qt-format
msgctxt "Creator|"
msgid "Cannot unmount partititons on device %1"
msgstr "Nem tudom a particiókat leválasztani az eszközről %1"

#, qt-format
msgctxt "Creator|"
msgid "Not enough space on %1 [%2 < %3]"
msgstr "Nincs elegendő hely a következő eszközön. %1 [%2 < %3]"

msgctxt "Creator|"
msgid "Syncing file system..."
msgstr "Fájlrendszer szinkronizálása..."

msgctxt "Creator|"
msgid "Writing done!"
msgstr "Írás kész!"

