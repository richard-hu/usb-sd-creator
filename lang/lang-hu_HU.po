msgctxt "QObject|"
msgid "Write Error"
msgstr "Írás hiba"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to write data to handle.\n"
"Error %1: %2"
msgstr ""
"Egy hiba történt az adat kiírása közben.\n"
"Hiba %1: %2"

msgctxt "QObject|"
msgid "Volume Error"
msgstr "Tároló Hiba"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to get a handle on the volume.\n"
"Error %1: %2"
msgstr ""
"Egy hiba történt egy kezelő kérése közben.\n"
"Error %1: %2"

msgctxt "QObject|"
msgid "Lock Error"
msgstr "Zárási Hiba"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to lock the volume.\n"
"Error %1: %2"
msgstr ""
"Egy hiba történt a tároló zárolása közben.\n"
"Hiba %1: %2"

msgctxt "QObject|"
msgid "Unlock Error"
msgstr "Feloldási Hiba"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to unlock the volume.\n"
"Error %1: %2"
msgstr ""
"Egy hiba történt a tároló feloldása közben\n"
"Hiba %1: %2"

msgctxt "QObject|"
msgid "Dismount Error"
msgstr "Leválasztási Hiba"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to dismount the volume.\n"
"Error %1: %2"
msgstr ""
"Egy hiba történt a tároló leválasztása közben.\n"
"Hiba %1: %2"
