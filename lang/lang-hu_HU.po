msgid "&Write"
msgstr "Írás"

msgctxt "Creator|"
msgid "&Help"
msgstr "Segítség"

msgctxt "Creator|"
msgid "Select &file"
msgstr "Tallózás"

msgctxt "Creator|"
msgid "&Show all"
msgstr "Mutasd mind"

msgctxt "Creator|"
msgid "Cance&l"
msgstr "Mégsem"

msgctxt "Creator|"
msgid "&Visit Website"
msgstr "Weboldal meglátogatása"

