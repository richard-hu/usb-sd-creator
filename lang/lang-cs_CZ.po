msgctxt "Creator|"
msgid "LibreELEC USB-SD Creator"
msgstr "LibreELEC USB-SD Creator"

msgctxt "Creator|"
msgid "Write image"
msgstr "Zapsat soubor img"

msgctxt "Creator|"
msgid "Select your USB stick or SD card"
msgstr "Vybrat USB disk nebo SD kartu"

msgctxt "Creator|"
msgid "Download or select a local image file"
msgstr "Stáhnout nebo vybrat lokální img soubor"

msgctxt "Creator|"
msgid "Eject device"
msgstr "Vysunout zařízení"

msgctxt "Creator|"
msgid "Load device"
msgstr "Načíst zařízení"

msgctxt "Creator|"
msgid ""
"Root privileges required to write image.\n"
"Run application with sudo."
msgstr ""
"K zapsání obrazu jsou potřeba práva root. Spustťe aplikaci pomocí sudo."

#, qt-format
msgctxt "Creator|"
msgid ""
"Version: %1\n"
"Build date: %2"
msgstr ""
"Verze: %1\n"
"Datum buildu: %2"

msgctxt "Creator|"
msgid "This software was created with love and released"
msgstr "Tento software byl vytvořen s láskou a uvolněn"

msgctxt "Creator|"
msgid "under GPLv2, using earlier work from RasPlex."
msgstr "pod GPlv2 díky předchozí práci z RasPlex"

msgctxt "Creator|"
msgid "For license, credits and history, please read:"
msgstr "Pro licenční ujednání, zásluhy a historii čtěte:"

msgctxt "Creator|"
msgid "If you enjoy using LibreELEC please consider a"
msgstr "Pokud se vám líbí LibreELEC zvažte"

msgctxt "Creator|"
msgid "donation to support the project."
msgstr "poskytnutí daru pro podporu projektu."

msgctxt "Creator|"
msgid "Click the logo below or donate"
msgstr "Klikněte na logo níže nebo darujte"

msgctxt "Creator|"
msgid "using Paypal to:"
msgstr "pomocí Paypalu na:"

msgctxt "Creator|"
msgid "[Stable]"
msgstr "[Stabilní]"

msgctxt "Creator|"
msgid "[Alpha]"
msgstr "[Alpha]"

msgctxt "Creator|"
msgid "[Beta]"
msgstr "[Beta]"

msgctxt "Creator|"
msgid "Download complete, syncing file..."
msgstr "Stažení dokončeno, probíhá synchronizace..."

msgctxt "Creator|"
msgid "Download complete, verifying checksum..."
msgstr "Stažení dokončeno, probíhá ověřování..."

msgctxt "Creator|"
msgid "Download complete, checksum ok."
msgstr "Stažení dokončeno, ověření proběhlo v pořádku."

msgctxt "Creator|"
msgid "Download complete, checksum not ok."
msgstr "Stažení dokončeno, ověření se nezdařilo."

#, qt-format
msgctxt "Creator|"
msgid "%1 seconds remaining - %2% at %3"
msgstr "%1 sekund zbývá - %2% na %3"

msgctxt "Creator|"
msgid "Download canceled."
msgstr "Stahování zrušeno."

msgctxt "Creator|"
msgid "Directory to store image file"
msgstr "Cesta k uložení img souboru"

msgctxt "Creator|"
msgid "Error"
msgstr "Chyba"

#, qt-format
msgctxt "Creator|"
msgid ""
"File \n"
"%1/%2\n"
"already exist."
msgstr ""
"Soubor \n"
"%1/%2\n"
"již existuje."

msgctxt "Creator|"
msgid "Do you want to overwrite?"
msgstr "Chcete jej přepsat?"

msgctxt "Creator|"
msgid "File already exists."
msgstr "Soubor již existuje."

msgctxt "Creator|"
msgid "Failed to open file for writing!"
msgstr "Nepodařilo se otevřít soubor k zapsání!"

msgctxt "Creator|"
msgid "Open image file"
msgstr "Otevřít soubor img"

msgctxt "Creator|"
msgid ""
"Compressed gz image (*img.gz);;Compressed zip image (*img.zip);;Uncompressed "
"image (*.img);;All files (*.*)"
msgstr ""
"Komprimovaný soubor gz (*img.gz);;Komprimovaný soubor zip "
"(*img.zip);;Nekomprimovaný soubor (*.img);;All files (*.*)"

msgctxt "Creator|"
msgid "Writing canceled."
msgstr "Zapisování bylo zrušeno"

msgctxt "Creator|"
msgid "Confirm write"
msgstr "Potvrďte zápis"

#, qt-format
msgctxt "Creator|"
msgid "Cannot unmount partititons on device %1"
msgstr "Nelze odebrat partions na zařízení %1"

#, qt-format
msgctxt "Creator|"
msgid "Not enough space on %1 [%2 < %3]"
msgstr "Nedostatek místa na %1 [%2 < %3]"

msgctxt "Creator|"
msgid "Syncing file system..."
msgstr "Synchronizace souborového systému..."

msgctxt "Creator|"
msgid "Writing done!"
msgstr "Zapisování dokončeno!"

#, qt-format
msgctxt "Creator|"
msgid "Error: %1"
msgstr "Chyba: %1"

#, qt-format
msgctxt "Creator|"
msgid ""
"Selected device: %1\n"
"Are you sure you want to write the image?\n"
"\n"
"Your USB-SD device will be wiped!"
msgstr ""
"Zvolené zařízení: %1\n"
"Určitě chcete zapsat img?\n"
"\n"
"Vaše USB zařízení bude zcela smazáno!"

msgctxt "Creator|"
msgid "Yes"
msgstr "Ano"

msgctxt "Creator|"
msgid "No"
msgstr "Ne"

msgctxt "Creator|"
msgid "OK"
msgstr "OK"

msgctxt "Creator|"
msgid "Update Notification"
msgstr "Upozornění na aktualizaci"

msgctxt "Creator|"
msgid "Select version"
msgstr "Vybrat verzi"

msgctxt "Creator|"
msgid "&Close"
msgstr "Zavřít"

msgctxt "Creator|"
msgid "&Download"
msgstr "Stáhnout"

msgctxt "Creator|"
msgid "&About"
msgstr "O programu"

msgctxt "Creator|"
msgid "&Write"
msgstr "Zapsat"

msgctxt "Creator|"
msgid "&Help"
msgstr "Pomoc"

msgctxt "Creator|"
msgid "Select &file"
msgstr "Vybrat soubor"

msgctxt "Creator|"
msgid "&Show all"
msgstr "Ukázat vše"

msgctxt "Creator|"
msgid "Cance&l"
msgstr "Zrušit"

msgctxt "Creator|"
msgid "&Visit Website"
msgstr "Navštivte webové stránky"

#, qt-format
msgctxt "Creator|"
msgid "LibreELEC USB-SD Creator %1 is available."
msgstr "LibreELEC USB-SD Creator %1 je k dispozici."

msgctxt "Creator|"
msgid "Select project"
msgstr "Vybrat projekt"

msgctxt "Creator|"
msgid "Select image"
msgstr "Vybrat soubor img"

msgctxt "Creator|"
msgid "Eject  Load         Remove"
msgstr ""

msgctxt "QObject|"
msgid "Write Error"
msgstr "Chyba zápisu"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to write data to handle.\n"
"Error %1: %2"
msgstr ""
"Při pokusu o zapsání dat nastala chyba.\n"
"Chyba %1: %2"

msgctxt "QObject|"
msgid "Volume Error"
msgstr "Chyba svazku"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to get a handle on the volume.\n"
"Error %1: %2"
msgstr ""
"Při pokusu spojení se svazkem nastala chyba.\n"
"Chyba %1: %2"

msgctxt "QObject|"
msgid "Lock Error"
msgstr "Chyba uzamčení"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to lock the volume.\n"
"Error %1: %2"
msgstr ""
"Při pokusu o uzamčení svazku nastala chyba.\n"
"Chyba %1: %2"

msgctxt "QObject|"
msgid "Unlock Error"
msgstr "Chyba odemčení"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to unlock the volume.\n"
"Error %1: %2"
msgstr ""
"Při pokusu o odemčení svazku nastala chyba.\n"
"Chyba %1: %2"

msgctxt "QObject|"
msgid "Dismount Error"
msgstr "Chyba odebrání"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to dismount the volume.\n"
"Error %1: %2"
msgstr ""
"Při pokusu o odebrání svazku nastala chyba.\n"
"Chyba %1: %2"
