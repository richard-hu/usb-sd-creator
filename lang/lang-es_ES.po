msgid "Load device"
msgstr "Cargar dispositivo"

msgctxt "Creator|"
msgid ""
"Root privileges required to write image.\n"
"Run application with sudo."
msgstr ""
"Se requiere privilegios de root para escribir imagen. \n"
"Ejecuta la aplicación con sudo."

#, qt-format
msgctxt "Creator|"
msgid ""
"Version: %1\n"
"Build date: %2"
msgstr ""
"Versión: %1\n"
