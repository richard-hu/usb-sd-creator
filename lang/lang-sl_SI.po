msgctxt "Creator|"
msgid "LibreELEC USB-SD Creator"
msgstr "LibreELEC USB-SD Creator"

msgctxt "Creator|"
msgid "Write image"
msgstr "Zapiši sliko diska"

msgctxt "Creator|"
msgid "Select your USB stick or SD card"
msgstr "Izberi USB ključ ali SD kartico"

msgctxt "Creator|"
msgid "Download or select a local image file"
msgstr "Prenesi ali izberi lokalno sliko diska"

msgctxt "Creator|"
msgid "Eject device"
msgstr "Eject device"

msgctxt "Creator|"
msgid "Load device"
msgstr "Load device"

msgctxt "Creator|"
msgid ""
"Root privileges required to write image.\n"
"Run application with sudo."
msgstr "Potrebne so root pravice za zapisovanje. Poženite aplikacijo s sudo."

#, qt-format
msgctxt "Creator|"
msgid ""
"Version: %1\n"
"Build date: %2"
msgstr ""
"Verzija: %1\n"
"Prevedeno: %2"

msgctxt "Creator|"
msgid "This software was created with love and released"
msgstr "Ta program je bil narejen z ljubeznijo in izdan"

msgctxt "Creator|"
msgid "under GPLv2, using earlier work from RasPlex."
msgstr "pod GPLv2 licenco iz prvotnega dela RasPlex."

msgctxt "Creator|"
msgid "For license, credits and history, please read:"
msgstr "Za licenco, zasluge in zgodovino preberite:"

msgctxt "Creator|"
msgid "If you enjoy using LibreELEC please consider a"
msgstr "Če z veseljem uporabljate LibreELEC razmislite o"

msgctxt "Creator|"
msgid "donation to support the project."
msgstr "donaciji in podprite projekt."

msgctxt "Creator|"
msgid "Click the logo below or donate"
msgstr "Kliknite spodnji logo ali donirajte"

msgctxt "Creator|"
msgid "using Paypal to:"
msgstr "z uporabo Paypala:"

msgctxt "Creator|"
msgid "[Stable]"
msgstr "[Stabilna]"

msgctxt "Creator|"
msgid "[Alpha]"
msgstr "[Alpha]"

msgctxt "Creator|"
msgid "[Beta]"
msgstr "[Beta]"

msgctxt "Creator|"
msgid "Download complete, syncing file..."
msgstr "Prenos končan, zapisujem datoteko..."

msgctxt "Creator|"
msgid "Download complete, verifying checksum..."
msgstr "Prenos končan, preverjam kontrolno vsoto..."

msgctxt "Creator|"
msgid "Download complete, checksum ok."
msgstr "Prenos končan, kontrolna vsota je ok..."

msgctxt "Creator|"
msgid "Download complete, checksum not ok."
msgstr "Prenos končan, kontrolna vsota ni ok..."

#, qt-format
msgctxt "Creator|"
msgid "%1 seconds remaining - %2% at %3"
msgstr "še %1 sekund - %2% z %3"

msgctxt "Creator|"
msgid "Download canceled."
msgstr "Prenos preklican."

msgctxt "Creator|"
msgid "Directory to store image file"
msgstr "Folder za shranjevanje slike diska"

msgctxt "Creator|"
msgid "Error"
msgstr "Napaka"

#, qt-format
msgctxt "Creator|"
msgid ""
"File \n"
"%1/%2\n"
"already exist."
msgstr ""
"Datoteka\n"
"%1/%2\n"
"že obstaja."

msgctxt "Creator|"
msgid "Do you want to overwrite?"
msgstr "Želite prepisati?"

msgctxt "Creator|"
msgid "File already exists."
msgstr "Datoteka že obstaja."

msgctxt "Creator|"
msgid "Failed to open file for writing!"
msgstr "Napaka pri odpiranju datoteke za zapisovanje!"

msgctxt "Creator|"
msgid "Open image file"
msgstr "Odpri sliko diska"

msgctxt "Creator|"
msgid ""
"Compressed gz image (*img.gz);;Compressed zip image (*img.zip);;Uncompressed "
"image (*.img);;All files (*.*)"
msgstr ""
"Stisnjena gz slika (*img.gz);;Stisnjena zip slika (*img.zip);;Nestisnjena "
"slika (*.img);;Vse datoteke (*.*)"

msgctxt "Creator|"
msgid "Writing canceled."
msgstr "Zapisovanje preklicano."

msgctxt "Creator|"
msgid "Confirm write"
msgstr "Potrdi zapisovanje"

#, qt-format
msgctxt "Creator|"
msgid "Cannot unmount partititons on device %1"
msgstr "Ne morem odklopiti particije na napravi %1"

#, qt-format
msgctxt "Creator|"
msgid "Not enough space on %1 [%2 < %3]"
msgstr "Ni dovolj prostora na %1 [%2 < %3]"

