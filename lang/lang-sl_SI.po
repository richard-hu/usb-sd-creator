msgctxt "QObject|"
msgid "Write Error"
msgstr "Napaka pri zapisovanju"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to write data to handle.\n"
"Error %1: %2"
msgstr ""
"Napaka pri zapisovanju podatkov v ročico.\n"
"Napaka %1: %2"

msgctxt "QObject|"
msgid "Volume Error"
msgstr "Napaka nosilca"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to get a handle on the volume.\n"
"Error %1: %2"
msgstr ""
"Napaka pri pridobivanju ročice za nosilec.\n"
"Napaka %1: %2"

msgctxt "QObject|"
msgid "Lock Error"
msgstr "Napaka pri zaklepanju"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to lock the volume.\n"
"Error %1: %2"
msgstr ""
"Napaka pri zaklepanju nosilca.\n"
"Napaka %1: %2"

msgctxt "QObject|"
msgid "Unlock Error"
msgstr "Napaka pri odklepanju"

