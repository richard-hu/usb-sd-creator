msgid "Syncing file system..."
msgstr "Sinhroniziram datotečni sistem..."

msgctxt "Creator|"
msgid "Writing done!"
msgstr "Zapisovanje končano!"

#, qt-format
msgctxt "Creator|"
msgid "Error: %1"
msgstr "Napaka: %1"

#, qt-format
msgctxt "Creator|"
msgid ""
"Selected device: %1\n"
"Are you sure you want to write the image?\n"
"\n"
"Your USB-SD device will be wiped!"
msgstr ""
"Izbrana naprava: %1\n"
"Želite res zapisati sliko diska?\n"
"\n"
"Vaša USB-SD naprava bo zbrisana!"

