msgctxt "Creator|"
msgid "LibreELEC USB-SD Creator %1 is available."
msgstr "LibreELEC USB-SD aplikacija %1 je dostupna."

msgctxt "Creator|"
msgid "Select project"
msgstr "Odaberi projekt"

msgctxt "Creator|"
msgid "Select image"
msgstr "Odaberite datoteku slike"

msgctxt "Creator|"
msgid "Eject  Load         Remove"
msgstr ""

msgctxt "QObject|"
msgid "Write Error"
msgstr "Pogreška kod zapisivanju"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to write data to handle.\n"
"Error %1: %2"
msgstr "Došlo je do pogreške prilikom pokušaja zapisa.\n"

msgctxt "QObject|"
msgid "Volume Error"
msgstr "Pogreška u nazivu"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to get a handle on the volume.\n"
"Error %1: %2"
msgstr "Došlo je do pogreške prilikom pristupa nazivu.\n"

msgctxt "QObject|"
msgid "Lock Error"
msgstr "Pogreška kod zaključavanja"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to lock the volume.\n"
"Error %1: %2"
msgstr "Došlo je do pogreške prilikom zaključavanja naziva.\n"

msgctxt "QObject|"
msgid "Unlock Error"
msgstr "Pogreška kod otključavanja"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to unlock the volume.\n"
"Error %1: %2"
msgstr "Došlo je do pogreške prilikom otključavanja naziva.\n"

msgctxt "QObject|"
msgid "Dismount Error"
msgstr "Pogreška kod odjave urešaja"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to dismount the volume.\n"
"Error %1: %2"
msgstr "Došlo je do pogreške prilikom odjave uređaja.\n"
