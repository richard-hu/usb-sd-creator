msgid "Eject  Load         Remove"
msgstr ""

msgctxt "QObject|"
msgid "Write Error"
msgstr "Chyba zápisu"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to write data to handle.\n"
"Error %1: %2"
msgstr ""
"Pri pokuse o zápis dát sa vyskytla chyba.\n"
"Chyba %1: %2"

msgctxt "QObject|"
msgid "Volume Error"
msgstr "Chyba jednotky"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to get a handle on the volume.\n"
"Error %1: %2"
msgstr ""
"Pri pokuse o spojenie so zväzkom sa vyskytla chyba.\n"
"Chyba %1: %2"

msgctxt "QObject|"
msgid "Lock Error"
msgstr "Chyba pri uzamykaní"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to lock the volume.\n"
"Error %1: %2"
msgstr ""
"Pri pokuse o uzamknutie zväzku sa vyskytla chyba.\n"
"Chyba %1: %2"

msgctxt "QObject|"
msgid "Unlock Error"
msgstr "Chyba pri odomykaní"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to unlock the volume.\n"
"Error %1: %2"
msgstr ""
"Pri pokuse o odomknutie zväzku sa vyskytla chyba.\n"
"Chyba %1: %2"

msgctxt "QObject|"
msgid "Dismount Error"
msgstr "Chyba pri odoberaní"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to dismount the volume.\n"
"Error %1: %2"
msgstr ""
"Pri pokuse o odobratie zväzku sa vyskytla chyba.\n"
"Chyba %1: %2"
