msgid "Select version"
msgstr "Vybrať verziu"

msgctxt "Creator|"
msgid "&Close"
msgstr "Zavrieť"

msgctxt "Creator|"
msgid "&Download"
msgstr "Stiahnúť"

msgctxt "Creator|"
msgid "&About"
msgstr "O programe"

msgctxt "Creator|"
msgid "&Write"
msgstr "Zapísať"

msgctxt "Creator|"
msgid "&Help"
msgstr "Pomoc"

msgctxt "Creator|"
msgid "Select &file"
msgstr "Vybrať súbor"

msgctxt "Creator|"
msgid "&Show all"
msgstr "Zobraziť všetko"

msgctxt "Creator|"
msgid "Cance&l"
msgstr "Zrušiť"

msgctxt "Creator|"
msgid "&Visit Website"
msgstr "Navštíviť webstránku"

#, qt-format
msgctxt "Creator|"
msgid "LibreELEC USB-SD Creator %1 is available."
msgstr "Je dostupný LibreELEC USB-SD Creator %1."

msgctxt "Creator|"
msgid "Select project"
msgstr "Vybrať projekt"

msgctxt "Creator|"
msgid "Select image"
msgstr "Vybrať obraz"

