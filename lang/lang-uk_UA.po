msgid "Eject  Load         Remove"
msgstr ""

msgctxt "QObject|"
msgid "Write Error"
msgstr "Помилка запису"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to write data to handle.\n"
"Error %1: %2"
msgstr ""
"Виникла помилка при спробі запису даних в хендл.\n"
"Помилка %1: %2"

msgctxt "QObject|"
msgid "Volume Error"
msgstr "Помилка тома"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to get a handle on the volume.\n"
"Error %1: %2"
msgstr ""
"Виникла помилка при спробі отримати хендл на томі.\n"
"Помилка %1: %2"

msgctxt "QObject|"
msgid "Lock Error"
msgstr "Помилка блокування"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to lock the volume.\n"
"Error %1: %2"
msgstr ""
"Виникла помилка при спробі блокування тома.\n"
"Помилка %1: %2"

msgctxt "QObject|"
msgid "Unlock Error"
msgstr "Помилка розблокування"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to unlock the volume.\n"
"Error %1: %2"
msgstr ""
"Виникла помилка при спробі розблокування тома.\n"
"Помилка %1: %2"

msgctxt "QObject|"
msgid "Dismount Error"
msgstr "Помикла розмонтування"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to dismount the volume.\n"
"Error %1: %2"
msgstr ""
"Виникла помилка при спробі розмонтування тома.\n"
"Помилка %1: %2"
